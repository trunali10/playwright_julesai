			"event": [
				{
					"listen": "test",
					"script": {
						"exec": [
							"pm.test(\"Login success - Status code is 200\", function () {",
							"  pm.response.to.have.status(200);",
							"});",
							"",
							"pm.test(\"Login success - Token is returned\", function () {",
							"  const res = pm.response.json();",
							"  pm.expect(res.data.login.token).to.be.a(\"string\");",
							"  pm.environment.set(\"token\", res.data.login.token); // Save token for next requests",
